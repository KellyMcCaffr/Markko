<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/holo_blue_bright"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="bob.markkouserloginsignup.productOverview"
    tools:showIn="@layout/activity_product_overview"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageView
        android:id="@+id/prodImSecondView2"
        android:layout_width="0dp"
        android:layout_height="75dp"
        android:layout_marginTop="5dp"
        android:onClick="uploadImage"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/prodImSecondView3"
        app:layout_constraintRight_toRightOf="@+id/prodImSecondView3"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageView
        android:id="@+id/prodImSecondView1"
        android:layout_width="0dp"
        android:layout_height="150dp"
        android:onClick="uploadImage"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintRight_toRightOf="@+id/prodName2View"
        android:layout_marginTop="46dp"
        app:layout_constraintLeft_toLeftOf="@+id/prodName2View"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/prodImSecondView3"
        android:layout_width="0dp"
        android:layout_height="75dp"
        android:layout_marginTop="10dp"
        android:onClick="uploadImage"
        app:layout_constraintTop_toBottomOf="@+id/prodImSecondView2"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/trialAllowBox2" />

    <ImageView
        android:id="@+id/prodImSecondView4"
        android:layout_width="0dp"
        android:layout_height="75dp"
        android:layout_marginEnd="7dp"
        android:layout_marginStart="9dp"
        android:onClick="uploadImage"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@+id/prodImSecondView3"
        app:layout_constraintLeft_toLeftOf="@+id/prodImSecondView3" />

    <ImageView
        android:id="@+id/prodImSecondView6"
        android:layout_width="0dp"
        android:layout_height="75dp"
        android:layout_marginEnd="7dp"
        android:onClick="uploadImage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintVertical_bias="0.011"
        app:layout_constraintRight_toLeftOf="@+id/prodImSecondView2"
        android:layout_marginStart="21dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/prodName2View" />

    <Button
        android:id="@+id/button27"
        android:layout_width="wrap_content"
        android:layout_height="52dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="12dp"
        android:background="@android:color/holo_red_light"
        android:onClick="deleteClicked"
        android:text="Delete"
        app:layout_constraintStart_toEndOf="@+id/button28"
        app:layout_constraintTop_toBottomOf="@+id/prodDescripView"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"/>

    <EditText
        android:id="@+id/prodDescripView"
        android:layout_width="224dp"
        android:layout_height="134dp"
        android:layout_marginBottom="80dp"
        android:layout_marginRight="48dp"
        android:background="@android:color/transparent"
        android:editable="true"
        android:ems="10"
        android:gravity="top|left"
        android:inputType="textPersonName"
        android:singleLine="false"
        android:textColor="@android:color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/prodImSecondView4"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"/>

    <EditText
        android:id="@+id/prodName2View"
        android:layout_width="150dp"
        android:layout_height="0dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:gravity="center_vertical|center_horizontal"
        android:inputType="textPersonName"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toTopOf="@+id/prodImSecondView1"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="7dp"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button28"
        android:layout_width="88dp"
        android:layout_height="52dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="12dp"
        android:background="@android:color/holo_orange_light"
        android:onClick="backButtonSelected"
        android:text="Back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/prodDescripView"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"/>

    <ImageView
        android:id="@+id/prodImSecondView5"
        android:layout_width="0dp"
        android:layout_height="75dp"
        android:onClick="uploadImage"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.206"
        app:layout_constraintRight_toRightOf="@+id/prodImSecondView6"
        app:layout_constraintLeft_toLeftOf="@+id/prodImSecondView6"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/textView94"
        android:layout_width="153dp"
        android:layout_height="43dp"
        android:layout_marginBottom="3dp"
        android:layout_marginRight="68dp"
        android:layout_marginTop="2dp"
        android:text="Description:"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/prodDescripView"
        app:layout_constraintRight_toLeftOf="@+id/button27"
        app:layout_constraintTop_toBottomOf="@+id/prodImSecondView4"
        app:layout_constraintVertical_bias="0.666"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"/>

    <EditText
        android:id="@+id/prodPriceBox2"
        android:layout_width="123dp"
        android:layout_height="40dp"
        android:layout_marginTop="14dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/prodDescripView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@+id/textView119"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/prodImSecondView4"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"/>

    <TextView
        android:id="@+id/textView119"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Price:"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="11dp"
        app:layout_constraintBottom_toBottomOf="@+id/prodPriceBox2"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="11dp"
        app:layout_constraintLeft_toRightOf="@+id/textView94" />

    <CheckBox
        android:id="@+id/purchaseAllowBox2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:layout_marginStart="16dp"
        android:text="Allow Purchases"
        app:layout_constraintBottom_toTopOf="@+id/textView94"
        app:layout_constraintStart_toStartOf="parent"
        tools:clickable="true"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"/>

    <CheckBox
        android:id="@+id/trialAllowBox2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:checked="false"
        android:elevation="1dp"
        android:text="Allow Trials"
        app:layout_constraintBaseline_toBaselineOf="@+id/purchaseAllowBox2"
        app:layout_constraintLeft_toRightOf="@+id/purchaseAllowBox2"
        tools:clickable="true"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="54dp"
        android:layout_height="52dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="36dp"
        android:onClick="checkmarkClicked"
        android:src="@drawable/checkmark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button28"
        />

</android.support.constraint.ConstraintLayout>
