<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="bob.markkouserloginsignup.swipeProductMoreImages"
    tools:showIn="@layout/activity_swipe_product_more_images">

    <ImageView
        android:id="@+id/pIm1"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_marginBottom="55dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toTopOf="@+id/pIm3"
        app:layout_constraintRight_toRightOf="@+id/pIm3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintRight_creator="1" />

    <ImageView
        android:id="@+id/pIm2"
        android:layout_width="125dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/pIm1"
        app:layout_constraintTop_toTopOf="@+id/pIm1"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintVertical_bias="1.0"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:id="@+id/pIm3"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_marginBottom="40dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toTopOf="@+id/pIm5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintLeft_creator="1" />

    <ImageView
        android:id="@+id/pIm4"
        android:layout_width="125dp"
        android:layout_height="125dp"
        android:layout_marginBottom="165dp"
        android:layout_marginTop="162dp"
        app:layout_constraintLeft_toLeftOf="@+id/pIm2"
        app:layout_constraintRight_toRightOf="@+id/pIm2"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/pIm6"
        app:layout_constraintTop_toTopOf="@+id/pIm2" />

    <ImageView
        android:id="@+id/pIm5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/pIm3"
        app:layout_constraintRight_toRightOf="@+id/pIm3"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintBottom_toBottomOf="@+id/pIm6"
        app:layout_constraintTop_toTopOf="@+id/pIm6" />

    <ImageView
        android:id="@+id/pIm6"
        android:layout_width="0dp"
        android:layout_height="125dp"
        android:layout_marginBottom="51dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/pIm4"
        app:layout_constraintRight_toRightOf="@+id/pIm4"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageView
        android:id="@+id/backArrow32"
        android:layout_width="82dp"
        android:layout_height="51dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="0dp"
        android:onClick="goBack"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pIm5"
        app:srcCompat="@drawable/arrowwhiteleft" />

</android.support.constraint.ConstraintLayout>
