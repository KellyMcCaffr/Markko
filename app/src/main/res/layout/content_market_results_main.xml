<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="bob.markkouserloginsignup.marketResultsMain"
    tools:showIn="@layout/activity_market_results_main">

    <Spinner
        android:id="@+id/businessMenu21"
        android:layout_width="51dp"
        android:layout_height="45dp"
        android:layout_marginEnd="2dp"
        android:layout_marginTop="5dp"
        android:background="@android:color/darker_gray"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/button38"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginBottom="50dp"
        android:layout_marginEnd="15dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="45dp"
        android:background="@android:color/holo_red_light"
        android:onClick="goToBusDataPieChart"
        android:text="Percent Likes"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/button40"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/businessMenu21"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/button40"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginEnd="15dp"
        android:layout_marginStart="15dp"
        android:background="@android:color/holo_blue_light"
        android:onClick="goToBusDataGraph"
        android:text="Total Likes"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/pr1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <Button
        android:id="@+id/pr1"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@android:color/holo_orange_light"
        android:onClick="goToSpecificProdInfoPage"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/pr2"
        app:layout_constraintLeft_toLeftOf="@+id/button40"
        app:layout_constraintRight_toRightOf="@+id/button40"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"/>

    <Button
        android:id="@+id/pr2"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@android:color/holo_green_light"
        android:onClick="goToSpecificProdInfoPage"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/pr3"
        app:layout_constraintLeft_toLeftOf="@+id/pr1"
        app:layout_constraintRight_toRightOf="@+id/pr1"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"/>

    <Button
        android:id="@+id/pr3"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@android:color/holo_red_dark"
        android:onClick="goToSpecificProdInfoPage"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/pr4"
        app:layout_constraintLeft_toLeftOf="@+id/pr2"
        app:layout_constraintRight_toRightOf="@+id/pr2"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"/>

    <Button
        android:id="@+id/pr4"
        android:layout_width="0dp"
        android:layout_height="51dp"
        android:background="@android:color/darker_gray"
        android:onClick="goToSpecificProdInfoPage"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/pr5"
        app:layout_constraintLeft_toLeftOf="@+id/pr3"
        app:layout_constraintRight_toRightOf="@+id/pr3"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"/>

    <Button
        android:id="@+id/pr5"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:background="@android:color/holo_purple"
        android:onClick="goToSpecificProdInfoPage"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/pr6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/pr4"
        app:layout_constraintRight_toRightOf="@+id/pr4"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"/>

    <Button
        android:id="@+id/pr6"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginBottom="16dp"
        android:background="@android:color/holo_green_light"
        android:onClick="goToSpecificProdInfoPage"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/pr5"
        app:layout_constraintRight_toRightOf="@+id/pr5"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"/>

    <TextView
        android:id="@+id/textView105"
        android:layout_width="211dp"
        android:layout_height="0dp"
        android:gravity="center_vertical|center_horizontal"
        android:text="All Consumer Data"
        android:textColor="@android:color/black"
        android:textSize="24sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/button38"
        android:layout_marginStart="45dp"
        app:layout_constraintTop_toBottomOf="@+id/businessMenu21"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="14dp"
        app:layout_constraintLeft_toLeftOf="@+id/button38" />
</android.support.constraint.ConstraintLayout>
