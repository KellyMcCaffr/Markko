<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="bob.markkouserloginsignup.busProfileProductsOverview"
    tools:showIn="@layout/activity_bus_profile_products_overview">

    <TextView
        android:id="@+id/textView80"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="Manage product profiles!"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="6dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/businessMenu"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="31dp"
        android:layout_marginRight="31dp" />

    <Button
        android:id="@+id/button18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="44dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:background="@android:color/holo_green_light"
        android:onClick="addProdButtonClicked"
        android:text="Add Product"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1" />

    <Spinner
        android:id="@+id/businessMenu"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:background="@android:color/darker_gray"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView80"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView81"
        android:layout_width="268dp"
        android:layout_height="48dp"
        android:layout_marginEnd="59dp"
        android:layout_marginRight="59dp"
        android:layout_marginTop="6dp"
        android:gravity="center"
        android:text="Manage product profiles!"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/prodOneTextView"
        android:layout_width="120dp"
        android:layout_height="0dp"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="13dp"
        android:background="@android:color/holo_orange_light"
        android:gravity="center"
        android:textColor="@android:color/white"
        app:layout_constraintHorizontal_bias="0.013"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1"
        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/prodTwoTextView"
        app:layout_constraintTop_toBottomOf="@+id/prodImView2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/prodTwoTextView"
        android:layout_width="120dp"
        android:layout_height="16dp"
        android:layout_marginLeft="68dp"
        android:background="@android:color/holo_orange_light"
        android:gravity="center"
        android:textColor="@android:color/white"
        app:layout_constraintLeft_toRightOf="@+id/prodOneTextView"
        android:layout_marginStart="68dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="156dp"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/prodThreeTextView"
        android:layout_width="120dp"
        android:layout_height="0dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="8dp"
        android:background="@android:color/holo_orange_light"
        android:gravity="center"
        android:textColor="@android:color/white"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/prodFourTextView"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="8dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/prodFourTextView"
        app:layout_constraintTop_toBottomOf="@+id/prodImView4" />

    <TextView
        android:id="@+id/prodFourTextView"
        android:layout_width="120dp"
        android:layout_height="16dp"
        android:layout_marginRight="36dp"
        android:background="@android:color/holo_orange_light"
        android:gravity="center"
        android:textColor="@android:color/white"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginEnd="36dp"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/prodImView4" />

    <TextView
        android:id="@+id/prodFiveTextView"
        android:layout_width="120dp"
        android:layout_height="16dp"
        android:layout_marginBottom="107dp"
        android:layout_marginLeft="16dp"
        android:background="@android:color/holo_orange_light"
        android:gravity="center"
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="16dp" />

    <TextView
        android:id="@+id/prodSixTextView"
        android:layout_width="120dp"
        android:layout_height="16dp"
        android:layout_marginBottom="107dp"
        android:layout_marginLeft="68dp"
        android:background="@android:color/holo_orange_light"
        android:gravity="center"
        android:textColor="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/prodFiveTextView"
        android:layout_marginStart="68dp" />

    <Button
        android:id="@+id/button19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/holo_orange_light"
        android:onClick="returnToMainBusPage"
        android:text="Back"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/prodFiveTextView"
        app:layout_constraintLeft_toRightOf="@+id/prodFiveTextView" />

    <ImageView
        android:id="@+id/prodImView2"
        android:layout_width="120dp"
        android:layout_height="84dp"
        android:layout_marginBottom="16dp"
        android:layout_marginTop="8dp"
        android:onClick="prodBoxClicked"
        app:layout_constraintBottom_toBottomOf="@+id/prodTwoTextView"
        app:layout_constraintLeft_toLeftOf="@+id/prodTwoTextView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintLeft_creator="1" />

    <ImageView
        android:id="@+id/prodImView1"
        android:layout_width="120dp"
        android:layout_height="84dp"
        android:layout_marginBottom="34dp"
        android:layout_marginLeft="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:onClick="prodBoxClicked"
        app:layout_constraintBottom_toTopOf="@+id/prodImView3"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView81"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@android:drawable/screen_background_dark_transparent" />

    <ImageView
        android:id="@+id/prodImView3"
        android:layout_width="120dp"
        android:layout_height="0dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="18dp"
        android:onClick="prodBoxClicked"
        app:layout_constraintBottom_toBottomOf="@+id/prodThreeTextView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/prodImView4"
        app:layout_constraintTop_toBottomOf="@+id/prodOneTextView"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageView
        android:id="@+id/prodImView4"
        android:layout_width="0dp"
        android:layout_height="84dp"
        android:layout_marginBottom="30dp"
        android:layout_marginTop="34dp"
        android:onClick="prodBoxClicked"
        app:layout_constraintBottom_toTopOf="@+id/prodImView6"
        app:layout_constraintLeft_toLeftOf="@+id/prodTwoTextView"
        app:layout_constraintRight_toRightOf="@+id/prodTwoTextView"
        app:layout_constraintTop_toTopOf="@+id/prodTwoTextView"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageView
        android:id="@+id/prodImView5"
        android:layout_width="0dp"
        android:layout_height="84dp"
        android:layout_marginBottom="16dp"
        android:onClick="prodBoxClicked"
        app:layout_constraintBottom_toBottomOf="@+id/prodFiveTextView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/prodFiveTextView"
        app:layout_constraintRight_toRightOf="@+id/prodFiveTextView"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <ImageView
        android:id="@+id/prodImView6"
        android:layout_width="0dp"
        android:layout_height="84dp"
        android:layout_marginBottom="16dp"
        android:onClick="prodBoxClicked"
        app:layout_constraintBottom_toBottomOf="@+id/prodSixTextView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@+id/prodFourTextView"
        app:layout_constraintRight_toRightOf="@+id/prodFourTextView"
        app:srcCompat="@android:drawable/screen_background_dark_transparent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

</android.support.constraint.ConstraintLayout>
