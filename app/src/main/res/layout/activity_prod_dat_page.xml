<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".prodDatPage">

    <TextView
        android:id="@+id/pName"
        android:layout_width="307dp"
        android:layout_height="20dp"
        android:layout_marginBottom="11dp"
        android:layout_marginStart="16dp"
        android:text="Product Name:"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/textView110"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintVertical_chainStyle="spread_inside"/>

    <TextView
        android:id="@+id/textView110"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Total Likes:"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/totLikes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pName"
        />

    <TextView
        android:id="@+id/totLikes"
        android:layout_width="300dp"
        android:layout_height="45dp"
        app:layout_constraintBottom_toTopOf="@+id/textView126"
        app:layout_constraintEnd_toEndOf="@+id/totDislikes"
        app:layout_constraintStart_toStartOf="@+id/textView110"
        app:layout_constraintTop_toBottomOf="@+id/textView110"
        />

    <TextView
        android:id="@+id/textView124"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ethnicity:"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/ethnicity"
        app:layout_constraintStart_toStartOf="@+id/percLikes"
        app:layout_constraintTop_toBottomOf="@+id/percLikes"
        />

    <TextView
        android:id="@+id/textView125"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="Religion:"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/religion"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ethnicity"
        />

    <TextView
        android:id="@+id/textView126"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:text="Dislikes:"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/totDislikes"
        app:layout_constraintStart_toStartOf="@+id/totLikes"
        app:layout_constraintTop_toBottomOf="@+id/totLikes"
        />

    <TextView
        android:id="@+id/textView127"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginBottom="2dp"
        android:text="Percent Likes:"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/percLikes"
        app:layout_constraintStart_toStartOf="@+id/percLikes"
        app:layout_constraintTop_toBottomOf="@+id/totDislikes"
        />

    <TextView
        android:id="@+id/totDislikes"
        android:layout_width="300dp"
        android:layout_height="45dp"
        app:layout_constraintBottom_toTopOf="@+id/textView127"
        app:layout_constraintEnd_toEndOf="@+id/ethnicity"
        app:layout_constraintStart_toStartOf="@+id/textView127"
        app:layout_constraintTop_toBottomOf="@+id/textView126"
        />

    <TextView
        android:id="@+id/percLikes"
        android:layout_width="301dp"
        android:layout_height="32dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toTopOf="@+id/textView124"
        app:layout_constraintEnd_toEndOf="@+id/pName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView127"
        />

    <TextView
        android:id="@+id/ethnicity"
        android:layout_width="300dp"
        android:layout_height="45dp"
        app:layout_constraintBottom_toTopOf="@+id/textView125"
        app:layout_constraintEnd_toEndOf="@+id/religion"
        app:layout_constraintStart_toStartOf="@+id/textView124"
        app:layout_constraintTop_toBottomOf="@+id/textView124"
        />

    <TextView
        android:id="@+id/religion"
        android:layout_width="300dp"
        android:layout_height="45dp"
        app:layout_constraintBottom_toTopOf="@+id/textView133"
        app:layout_constraintEnd_toEndOf="@+id/age"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView125"
        app:layout_constraintTop_toBottomOf="@+id/textView125"/>

    <TextView
        android:id="@+id/textView133"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Age:"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/age"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/religion"
        />

    <TextView
        android:id="@+id/age"
        android:layout_width="300dp"
        android:layout_height="45dp"
        app:layout_constraintBottom_toTopOf="@+id/textView135"
        app:layout_constraintStart_toStartOf="@+id/textView133"
        app:layout_constraintTop_toBottomOf="@+id/textView133"
        />

    <TextView
        android:id="@+id/textView135"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Education Level:"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/education"
        app:layout_constraintStart_toStartOf="@+id/age"
        app:layout_constraintTop_toBottomOf="@+id/age"
        />

    <TextView
        android:id="@+id/education"
        android:layout_width="300dp"
        android:layout_height="45dp"
        android:layout_marginBottom="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/age"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView135"
        app:layout_constraintTop_toBottomOf="@+id/textView135"/>

    <TextView
        android:id="@+id/textView108"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:text="Gender:"
        android:textColor="@android:color/black"
        app:layout_constraintBottom_toTopOf="@+id/gender"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:id="@+id/gender"
        android:layout_width="313dp"
        android:layout_height="27dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />

</android.support.constraint.ConstraintLayout>