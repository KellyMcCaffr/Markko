<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="bob.markkouserloginsignup.viewAccountsDatabase2"
    tools:showIn="@layout/activity_view_accounts_database2">

    <TextView
        android:id="@+id/textView45"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Gender"
        android:textColor="@android:color/black"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="4dp" />

    <TextView
        android:id="@+id/textView46"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="Age"
        android:textColor="@android:color/black"
        app:layout_constraintRight_toLeftOf="@+id/textView47"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/ageViewBox"
        android:layout_marginStart="11dp"
        android:layout_marginEnd="12dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/textView45"
        app:layout_constraintLeft_toRightOf="@+id/textView45"
        android:layout_marginLeft="11dp"
        android:layout_marginRight="12dp" />

    <TextView
        android:id="@+id/textView47"
        android:layout_width="80dp"
        android:layout_height="0dp"
        android:text="Race"
        android:textColor="@android:color/black"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/genderViewBox"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView46" />

    <TextView
        android:id="@+id/textView50"
        android:layout_width="86dp"
        android:layout_height="0dp"
        android:text="Religion"
        android:textColor="@android:color/black"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/religionViewBox"
        android:layout_marginEnd="37dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView47"
        android:layout_marginRight="37dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:layout_marginEnd="29dp"
        android:layout_marginRight="29dp"
        android:onClick="nextViewDBPage"
        android:text="Next"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="@+id/raceViewBox"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintRight_creator="1" />

    <TextView
        android:id="@+id/genderViewBox"
        android:layout_width="54dp"
        android:layout_height="433dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/ageViewBox"
        android:layout_marginEnd="7dp"
        android:layout_marginTop="1dp"
        app:layout_constraintTop_toBottomOf="@+id/textView45"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="1dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView45"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_editor_absoluteY="28dp"
        android:layout_marginLeft="0dp" />

    <TextView
        android:id="@+id/ageViewBox"
        android:layout_width="72dp"
        android:layout_height="438dp"
        android:layout_marginBottom="2dp"
        android:layout_marginEnd="10dp"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp"
        app:layout_constraintBottom_toBottomOf="@+id/religionViewBox"
        app:layout_constraintLeft_toRightOf="@+id/textView45"
        app:layout_constraintTop_toBottomOf="@+id/textView45"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        tools:layout_editor_absoluteY="24dp" />

    <TextView
        android:id="@+id/raceViewBox"
        android:layout_width="0dp"
        android:layout_height="440dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textView47"
        app:layout_constraintTop_toBottomOf="@+id/textView47"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView47"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/button5" />

    <TextView
        android:id="@+id/religionViewBox"
        android:layout_width="0dp"
        android:layout_height="443dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/textView50"
        android:layout_marginTop="21dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView50"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>
